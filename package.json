{
  "name": "vandelay",
  "version": "6.4.7",
  "description": "Imports, exports, and ETL",
  "main": "dist/index.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staeco/vandelay.git"
  },
  "contributors": [
    "Contra <yo@contra.io> (http://contra.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staeco/vandelay/issues"
  },
  "homepage": "https://github.com/staeco/vandelay#readme",
  "files": [
    "dist"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "skip-full": true,
    "check-coverage": true
  },
  "scripts": {
    "preversion": "npm run clean && npm run build",
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "test": "NODE_ENV=testing nyc mocha --require @babel/register --recursive --reporter spec --bail --exit --timeout 36000 && npm run-script lint",
    "test:debug": "NODE_ENV=testing nyc mocha --inspect-brk=0.0.0.0 --require @babel/register --recursive --reporter spec --bail --timeout 36000"
  },
  "resolutions": {
    "end-of-stream": "github:isaacs/end-of-stream#custom-stream-premature-close-fix"
  },
  "engines" : {
    "node" : ">=12"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@stae/babel-node": "^0.1.0",
    "@stae/linters": "^0.1.2",
    "babel-plugin-istanbul": "^6.0.0",
    "body-parser": "^1.18.3",
    "eslint": "^6.0.0",
    "eslint-cli": "^1.0.0",
    "express": "^4.16.3",
    "get-port": "^5.0.0",
    "graceful-fs": "^4.1.11",
    "into-stream": "^5.0.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "range-parser": "^1.2.0",
    "rimraf": "^3.0.0",
    "should": "^13.0.0",
    "tempfile": "^3.0.0",
    "vandelay-es6": "^2.0.0",
    "xlsx": "^0.15.3"
  },
  "dependencies": {
    "JSONStream": "^1.3.2",
    "camelcase": "^5.0.0",
    "core-js": "3.0.0",
    "csv-parser": "^2.0.0",
    "duplexify": "^4.0.0",
    "end-of-stream": "github:isaacs/end-of-stream#custom-stream-premature-close-fix",
    "exceljs-transform-stream": "^1.0.0",
    "get-stream": "^5.0.0",
    "got-resume": "github:contra/got-resume",
    "gtfs-stream": "^2.1.0",
    "is-plain-obj": "^2.0.0",
    "lodash.clone": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.pickby": "^4.6.0",
    "merge2": "^1.2.3",
    "moment": "^2.24.0",
    "ndjson": "^1.5.0",
    "object-transform-stack": "^3.0.0",
    "parse-decimal-number": "^1.0.0",
    "pump": "^3.0.0",
    "pumpify": "^2.0.0",
    "qs": "^6.6.0",
    "remove-bom-stream": "^1.2.0",
    "superagent": "^5.0.0",
    "through2": "^3.0.0",
    "through2-concurrent": "^2.0.0",
    "unzipper": "^0.10.1",
    "url-template": "^2.0.8",
    "verrazzano": "^2.0.0",
    "vm2": "^3.6.0",
    "wkx": "^0.4.6",
    "xml2js": "^0.4.19"
  }
}
